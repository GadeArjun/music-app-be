{
  "builds": [
    {
      "src": "/build/index.html",
      "use": "@vercel/static"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    },
    {
      "src": "/_90ssongs/",
      "dest": "/index.js"
    },
    {
      "src": "/newsongs/",
      "dest": "/index.js"
    },
    {
      "src": "/hindisongs/",
      "dest": "/index.js"
    },
    {
      "src": "/trendingsongs/",
      "dest": "/index.js"
    },
    {
      "src": "/marathisongs/",
      "dest": "/index.js"
    }
  ]
}
